<Defs>
    <TerrainDef ParentName="NaturalTerrainBase">
        <defName>GeonosisSand</defName>
        <label>geonosis sand</label>
        <texturePath>Terrain/GeonosisSand</texturePath>
        <edgeType>FadeRough</edgeType>
        <renderPrecedence>350</renderPrecedence>
        <pathCost>4</pathCost>
        <statBases>
            <Beauty>-1</Beauty>
            <Cleanliness>-1</Cleanliness>
        </statBases>
        <affordances>
            <li>Light</li>
            <li>Medium</li>
            <li>Heavy</li>
            <li>Diggable</li>
        </affordances>
        <generatedFilth>Filth_Sand</generatedFilth>
        <fertility>0.10</fertility>
        <takeFootprints>True</takeFootprints>
        <tools>
            <li>
                <label>sand</label>
                <capacities>
                    <li>KickMaterialInEyes</li>
                </capacities>
                <hediff>SandInEyes</hediff>
                <cooldownTime>1.5</cooldownTime>
            </li>
        </tools>
    </TerrainDef>
	
