	<ThingDef ParentName="AnimalThingBase">
        <defName>Krill</defName>
        <label>krill</label>
        <description>Krill are small crustaceans and are found in all the world's oceans.</description>
        <statBases>
            <MoveSpeed>0</MoveSpeed>
            <!--<SwimSpeed>4</SwimSpeed>-->
            <MarketValue>15</MarketValue>
            <ComfyTemperatureMin>-30</ComfyTemperatureMin>
            <MeatAmount>10</MeatAmount>
            <LeatherAmount>0</LeatherAmount>
        </statBases>
        <race>
            <wildBiomes>
                <Lake>0.9</Lake>
            </wildBiomes>
            <herdAnimal>true</herdAnimal>
            <body>KrillAnimalBody</body>
            <baseBodySize>0.15</baseBodySize>
            <baseHungerRate>0</baseHungerRate>
            <baseHealthScale>0.1</baseHealthScale>
            <foodType>VegetarianRoughAnimal</foodType>
            <leatherDef>Leather_Krill</leatherDef>
            <meatLabel>Krill</meatLabel>
            <wildness>0.50</wildness>
            <trainability>None</trainability>
            <manhunterOnTameFailChance>0</manhunterOnTameFailChance>
            <manhunterOnDamageChance>0</manhunterOnDamageChance>
            <mateMtbHours>8</mateMtbHours>
            <gestationPeriodDays>16</gestationPeriodDays>
            <!--<litterSizeCurve>
        <points>
          <li>(1.0, 0)</li>
          <li>(1.5, 1)</li>
          <li>(2.0, 1)</li>
          <li>(2.5, 0)</li>
        </points>
      </litterSizeCurve>-->
            <lifeExpectancy>1.5</lifeExpectancy>
            <lifeStageAges>
                <li>
                    <def>AnimalBaby</def>
                    <minAge>0</minAge>
                </li>
                <li>
                    <def>AnimalJuvenile</def>
                    <minAge>0.07</minAge>
                </li>
                <li>
                    <def>AnimalAdult</def>
                    <minAge>0.3</minAge>
                    <soundWounded>Pawn_Boomrat_Wounded</soundWounded>
                    <soundDeath>Pawn_Boomrat_Death</soundDeath>
                    <soundCall>Pawn_Boomrat_Call</soundCall>
                    <soundAngry>Pawn_Boomrat_Angry</soundAngry>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
        </race>
        <tradeTags>
            <li>AnimalUncommon</li>
